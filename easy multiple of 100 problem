//Multiple of 100

//Create a function that takes an integer and returns true if it's divisible by 100, otherwise return false.
//Examples

//divisible(1) ➞ false

//divisible(1000) ➞ true

//divisible(100) ➞ true

function divisible(num) {
	var check = num%100;
	if (check ===0) {
		return true
	}
	else return false;
}
